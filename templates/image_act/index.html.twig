{% extends 'base.html.twig' %}

{% block title %}ImageAct index{% endblock %}

{% block body %}
    <h1>ImageAct index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>IdImage</th>
                <th>ImageAct</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for image_act in image_acts %}
            <tr>
                <td>{{ image_act.idImage }}</td>
                <td>{{ image_act.imageAct }}</td>
                <td>
                    <a href="{{ path('image_act_show', {'idImage': image_act.idImage}) }}">show</a>
                    <a href="{{ path('image_act_edit', {'idImage': image_act.idImage}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('image_act_new') }}">Create new</a>
{% endblock %}
